{
  "version": 2,
  "functions": {
    "api/health.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "api/test.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "api/version.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.php"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.php"
    },
    {
      "src": "/api/version",
      "dest": "/api/version.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}